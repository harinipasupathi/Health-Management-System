INSERT INTO Medical_Records (Patient_ID, Doctor_ID, Record_Date, Diagnosis, Treatment, Prescription, Notes) 
VALUES (
    (SELECT Patient_ID FROM Patients WHERE First_Name = 'Chloe' AND Last_Name = 'Brown'),
    1, -- Reusing Doctor_ID 1
    TO_DATE('2023-09-30', 'YYYY-MM-DD'),
    'Flu-like Symptoms',
    'Rest and hydration',
    'Oseltamivir 75mg',
    'Monitor symptoms and report back.'
);

